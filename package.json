{
	"name": "bx-app-quotation-generator",
	"version": "0.0.0",
	"description": "Bitrix24 Local App with Cloudflare Worker",
	"main": "src/worker.js",
	"private": true,
	"scripts": {
		"deploy": "wrangler deploy",
		"deploy:dev": "wrangler deploy --env dev",
		"deploy:prod": "wrangler deploy",
		"dev": "wrangler dev --env dev",
		"dev:prod": "wrangler dev",
		"start": "wrangler dev --env dev",
		"test": "vitest",
		"tail": "wrangler tail",
		"tail:dev": "wrangler tail --env dev",
		"tail:prod": "wrangler tail"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.8.19",
		"vitest": "~3.2.0",
		"wrangler": "^4.33.1"
	}
}
